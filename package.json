{
  "name": "3d-asset-forge",
  "version": "1.0.0",
  "description": "AI-powered 3D asset generation system",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:backend": "concurrently \"npm run dev:api\" \"npm run dev:images\"",
    "dev:api": "npm run build:services && node server/api.mjs",
    "dev:images": "node scripts/start-image-server.mjs",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:backend\"",
    "build": "npm run clean && tsc && vite build",
    "build:services": "node scripts/build-services.mjs",
    "start": "npm run start:backend",
    "start:backend": "concurrently \"npm run start:api\" \"npm run start:image-server\"",
    "start:api": "npm run build:services && node server/api.mjs",
    "start:image-server": "node scripts/start-image-server.mjs",
    "clean": "rm -rf dist",
    "lint": "eslint . --ext .ts,.tsx",
    "preview": "vite preview",
    "assets:audit": "npx tsx scripts/audit-assets.ts",
    "assets:normalize": "npx tsx scripts/normalize-all-assets.ts",
    "check:deps": "depcheck",
    "check:all": "knip",
    "count:lines": "node scripts/count-lines.mjs",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "keywords": [
    "ai",
    "3d",
    "generation",
    "meshy",
    "gpt-4",
    "rpg",
    "game-assets"
  ],
  "author": "Lucid",
  "license": "MIT",
  "dependencies": {
    "@mediapipe/hands": "^0.4.1675469240",
    "@react-three/drei": "^9.109.5",
    "@react-three/fiber": "^8.16.8",
    "@tensorflow-models/hand-pose-detection": "^2.0.1",
    "@tensorflow/tfjs": "^4.22.0",
    "@tensorflow/tfjs-backend-webgl": "^4.22.0",
    "chalk": "^4.1.2",
    "clsx": "^2.0.0",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "immer": "^10.1.1",
    "lucide-react": "^0.525.0",
    "node-fetch": "^3.3.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^3.3.1",
    "three": "^0.178.0",
    "three-mesh-bvh": "^0.9.1",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@types/node": "^20.19.8",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/three": "^0.178.1",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.16",
    "depcheck": "^1.4.7",
    "eslint": "^9.33.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "knip": "^5.62.0",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.3.3",
    "vite": "^6.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18.0.0"
  }
}
