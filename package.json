{
  "name": "@hyperscape/ai-creation",
  "version": "1.0.0",
  "description": "AI-powered 3D asset generation system for Hyperscape RPG",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:backend": "concurrently \"npm run dev:api\" \"npm run dev:images\"",
    "dev:api": "npm run build:services && node server/api.mjs",
    "dev:images": "node scripts/start-image-server.mjs",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:backend\"",
    "build": "npm run clean && tsc && vite build",
    "build:services": "node scripts/build-services.mjs",
    "start": "npm run start:backend",
    "start:backend": "concurrently \"npm run start:api\" \"npm run start:image-server\"",
    "start:api": "npm run build:services && node server/api.mjs",
    "start:image-server": "node scripts/start-image-server.mjs",
    "clean": "rm -rf dist",
    "lint": "tsc --noEmit",
    "preview": "vite preview",
    "assets:audit": "npx tsx scripts/audit-assets.ts",
    "assets:normalize": "npx tsx scripts/normalize-all-assets.ts",
    "assets:extract-tpose": "npx tsx scripts/extract-tpose-simple-cli.ts"
  },
  "keywords": [
    "ai",
    "3d",
    "generation",
    "meshy",
    "gpt-4",
    "rpg",
    "game-assets"
  ],
  "author": "Hyperscape Team",
  "license": "MIT",
  "dependencies": {
    "@mediapipe/hands": "^0.4.1675469240",
    "@react-three/drei": "^9.109.5",
    "@react-three/fiber": "^8.16.8",
    "@tensorflow-models/hand-pose-detection": "^2.0.1",
    "@tensorflow/tfjs": "^4.22.0",
    "@tensorflow/tfjs-backend-webgl": "^4.22.0",
    "chalk": "^4.1.2",
    "clsx": "^2.0.0",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "immer": "^10.1.1",
    "lucide-react": "^0.525.0",
    "node-cache": "^5.1.2",
    "node-fetch": "^3.3.2",
    "openai": "^4.24.0",
    "p-limit": "^5.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sharp": "^0.33.1",
    "three": "^0.178.0",
    "ws": "^8.16.0",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.19.8",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/three": "^0.178.1",
    "@types/ws": "^8.5.10",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.16",
    "jest": "^29.7.0",
    "playwright": "^1.40.0",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3",
    "vite": "^6.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18.0.0"
  }
}
